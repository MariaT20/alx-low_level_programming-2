#include <stdio.h>
/**
 * main - prints the sze of data types
 * Return: 0
 */
int main(void)
{
int;
int long;
char;
long long int;
float;

printf("%lu,sizeof int\n");
printf("%lu,sizeof int long\n");
printf("%lu,sizeof char\n");
printf("%lu,sizeof long long int\n");
printf("%lu,sizeof float\n");
return (0);
}

